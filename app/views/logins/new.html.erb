<div>
  <% if @current_user %>
    <%= @current_user_path %>
  <% end %>
</div>

<%= form_tag signin_path, method: "POST" %>

 <%= label_tag("Username") %>
 <%= text_field_tag :username %><br/>
 <%= label_tag("Password") %>
 <%= password_field_tag :password %><br/>
 
 <%= submit_tag("Login") %><br/>

 <%= link_to "Don't have an account yet?", new_user_path %>

